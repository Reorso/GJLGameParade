--- !u!114 &-2313070533747158448
MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 7212457427814139539}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: dbacdf024357cfb4389077f2a64e0e09, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  facingUp: 0
  anim: {fileID: 0}
  graphicObj: {fileID: 2572496268493090062}
